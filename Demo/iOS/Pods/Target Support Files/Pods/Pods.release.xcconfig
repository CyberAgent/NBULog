GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CocoaLumberjack" "${PODS_ROOT}/Headers/Public/LumberjackConsole" "${PODS_ROOT}/Headers/Public/NBUCore" "${PODS_ROOT}/Headers/Public/NBULog"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/LumberjackConsole" -isystem "${PODS_ROOT}/Headers/Public/NBUCore" -isystem "${PODS_ROOT}/Headers/Public/NBULog"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CocoaLumberjack" -l"LumberjackConsole" -l"NBUCore" -l"NBULog"
PODS_ROOT = ${SRCROOT}/Pods